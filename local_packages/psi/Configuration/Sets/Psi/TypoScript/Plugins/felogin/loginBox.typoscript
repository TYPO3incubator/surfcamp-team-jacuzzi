lib.loginBox = USER
lib.loginBox {
  userFunc = TYPO3\CMS\Extbase\Core\Bootstrap->run
  extensionName = Felogin
  pluginName = Login
  settings < plugin.tx_felogin_login.settings
  settings {
    redirectMode = logout
    redirectPageLogout = 1
  }
  view < plugin.tx_felogin_login.view
  view {
      templateRootPaths {
        10 = {$plugin.tx_felogin_login.view.templateRootPath}
        20 = EXT:psi/Resources/Private/Templates/Plugins/FeloginHeader/
      }
  }
}
